
config gpio 'general'
	option debug '0'
	option isActive '0'

config gpio 'IO_0'
	option status 'enable'
	option direction 'in'
	option value '0'		
	option trigger 'rising'
	option debounce_ms '100'
	option action_command 'echo "IO0 detected"'
	option hw_info 'usb-cp2112'

config gpio 'IO_1'
  option status 'enable'
  option direction 'in'
  option value '0'    
  option trigger 'rising'
  option debounce_ms '100'
  option action_command 'echo "IO1 detected"'
  option hw_info 'usb-cp2112'

