
config gpio 'general'
	option debug '0'
  option isActive '1'

config gpio 'IO0'
	option status 'enable'
  option direction 'OUT'
  option trigger 'RISE'
  option value 'LOW'	
	option debounce_ms '100'
	option action_command 'touch /usr/lib/lua/tsmgpio/IO0_detect'
	option hw_info 'usb-cp2112'
  option action_type 'command'

config gpio 'IO1'
  option status 'enable'
  option direction 'OUT'
  option trigger 'RISE'
  option value 'LOW'  
  option debounce_ms '100'
  option action_command 'touch /usr/lib/lua/tsmgpio/IO1_detect'
  option hw_info 'usb-cp2112'
  option action_type 'command'

config gpio 'IO2'
  option status 'disable'
  option direction 'IN'
  option trigger 'RISE'
  option value 'LOW'  
  option debounce_ms '100'
  option action_command 'touch /usr/lib/lua/tsmgpio/IO2_detect'
  option hw_info 'usb-cp2112'
  option action_type 'command'

config gpio 'IO3'
