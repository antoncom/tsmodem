
config adapter 'template'
	option name '0'
	option gate_address '0'
	option balance_ussd '0'
	option balance_mask '0'
	option balance_example_message '0'

config adapter '00000'
	option editable '0'
	option name 'AUTO'
	option code 'auto'
	option gate_address 'internet'
	option balance_ussd 'auto'
	option balance_mask '__RUB__'
	option balance_last_message 'Unexpected Data Value'

